<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.examenpracticoud2.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText" text="Simulación de Tráfico"/>

    <Button text="Añadir Coche" onAction="#onHelloButtonClick"/>

    <Pane fx:id="panelCarretera" prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: #333;"/>
</VBox>